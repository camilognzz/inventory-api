{
  "name": "inventory-api",
  "version": "1.0.0",
  "description": "Sistema de inventario",
  "main": "src/main/app.js",
  "scripts": {
    "start": "node src/main/app.js",
    "dev": "nodemon src/main/app.js",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:setup": "npm run migrate && npm run seed",
    "apidoc": "apidoc -i src/ -o public/apidoc/"
  },
  "dependencies": {
    "apidoc": "^0.53.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1"
  }
}
